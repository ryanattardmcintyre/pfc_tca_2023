@model ErrorViewModel
@{
    ViewData["Title"] = "Error";
}

<h1>This is an Error Page. Something may have gone wrong</h1>






<img src="https://thumbs.dreamstime.com/b/error-rubber-stamp-word-error-inside-illustration-109026446.jpg" style="width:100%; height: 200px;" /> 

@if (TempData["Error"] != null)
{
    <div class="alert alert-danger" role="alert">
        @TempData["Error"].ToString()
    </div>
} 